/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=nots);
    And(a=a, b=nots, out=as);
    And(a=b, b=sel, out=bs);
    Or(a=as, b=bs, out=out);
}